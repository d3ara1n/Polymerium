{
    "productName": "polymerium",
    "version": "0.1.0",
    "identifier": "io.github.polymerium",
    "build": {
        "frontendDist": "../.output/public",
        "devUrl": "http://localhost:3060",
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build"
    },
    "app": {
        "windows": [
            {
                "title": "Polymerium DEV",
                "width": 800,
                "height": 600,
                "resizable": true,
                "transparent": true,
                "decorations": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json"
}
