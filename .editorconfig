resharper_cpp_insert_final_newline = true
resharper_csharp_align_first_arg_by_paren = true
resharper_csharp_empty_block_style = together_same_line
resharper_csharp_keep_existing_arrangement = false
resharper_csharp_max_enum_members_on_line = 10000
resharper_csharp_outdent_commas = true
resharper_csharp_outdent_dots = true
resharper_csharp_stick_comment = false
resharper_csharp_wrap_after_declaration_lpar = true
