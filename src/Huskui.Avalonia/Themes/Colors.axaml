<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="Colors.White.axaml" />
        <ResourceInclude Source="Colors.Black.axaml" />
        <ResourceInclude Source="Colors.Gray.Neutral.axaml" />
        <ResourceInclude Source="Colors.Accent.Amber.axaml" />
        <ResourceInclude Source="Colors.Success.Grass.axaml" />
        <ResourceInclude Source="Colors.Warning.Orange.axaml" />
        <ResourceInclude Source="Colors.Danger.Tomato.axaml" />
    </ResourceDictionary.MergedDictionaries>
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <Color x:Key="TransparentColor">#00ffffff</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <Color x:Key="TransparentColor">#00000000</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    
    
    

    <!-- Naming: OwnerLevelSubtle(Accent)PropertyType -->
    
    <SolidColorBrush x:Key="TransparentBrush" Color="{DynamicResource TransparentColor}" />
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="{DynamicResource Gray4Color}" />

    <!-- 颜色固定，用作按钮遮罩 -->
    <SolidColorBrush x:Key="OverlayLighter1BackgroundBrush" Color="{DynamicResource White1Color}" />
    <SolidColorBrush x:Key="OverlayLighter2BackgroundBrush" Color="{DynamicResource White2Color}" />
    <SolidColorBrush x:Key="OverlayLighter6BackgroundBrush" Color="{DynamicResource White6Color}" />
    <SolidColorBrush x:Key="OverlayDarker1BackgroundBrush" Color="{DynamicResource Black1Color}" />
    <SolidColorBrush x:Key="OverlayDarker2BackgroundBrush" Color="{DynamicResource Black2Color}" />
    <SolidColorBrush x:Key="OverlayDarker7BackgroundBrush" Color="{DynamicResource Black7Color}" />
    <!-- 颜色动态，用作按钮遮罩 -->
    <SolidColorBrush x:Key="OverlayHalfBackgroundBrush" Color="{DynamicResource Black1Color}" />
    <SolidColorBrush x:Key="OverlayFullBackgroundBrush" Color="{DynamicResource Black2Color}" />

    <!-- 颜色会相对亮暗主题改变，用作层叠项目背景 -->
    <SolidColorBrush x:Key="LayerSolidBackgroundBrush" Color="{DynamicResource Gray1Color}" />
    <SolidColorBrush x:Key="Layer0BackgroundBrush" Color="{DynamicResource White10Color}" />
    <SolidColorBrush x:Key="Layer1BackgroundBrush" Color="{DynamicResource Black1Color}" />
    <SolidColorBrush x:Key="Layer1ReversedBackgroundBrush" Color="{DynamicResource White1Color}" />
    <SolidColorBrush x:Key="Layer2BackgroundBrush" Color="{DynamicResource Black2Color}" />
    <SolidColorBrush x:Key="Layer2ReversedBackgroundBrush" Color="{DynamicResource White2Color}" />
    <SolidColorBrush x:Key="Layer3BackgroundBrush" Color="{DynamicResource Black3Color}" />
    <SolidColorBrush x:Key="Layer3ReversedBackgroundBrush" Color="{DynamicResource White3Color}" />
    <SolidColorBrush x:Key="Layer7BackgroundBrush" Color="{DynamicResource Black7Color}" />

    <SolidColorBrush x:Key="LayerSmokeBackgroundBrush" Color="{DynamicResource Black7Color}" />
    <SolidColorBrush x:Key="LayerSmokeReversedBackgroundBrush" Color="{DynamicResource White7Color}" />

    <SolidColorBrush x:Key="Card1BackgroundBrush" Color="{DynamicResource Gray2Color}" />
    <SolidColorBrush x:Key="Card2BackgroundBrush" Color="{DynamicResource Gray1Color}" />
    <SolidColorBrush x:Key="Card3BackgroundBrush" Color="{DynamicResource Gray3Color}" />

    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="{DynamicResource Gray5Color}" />
    <SolidColorBrush x:Key="ControlTranslucentBackgroundBrush" Color="{DynamicResource Gray6Color}"
                     Opacity="0.4" />
    <SolidColorBrush x:Key="ControlTranslucentOverlayBackgroundBrush" Color="{DynamicResource Gray6Color}"
                     Opacity="0.1" />
    <SolidColorBrush x:Key="ControlAccentBackgroundBrush" Color="{DynamicResource Accent9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentAccentBackgroundBrush" Color="{DynamicResource Accent6Color}"
                     Opacity="0.4" />
    <SolidColorBrush x:Key="ControlTranslucentOverlayAccentBackgroundBrush"
                     Color="{DynamicResource Accent9Color}"
                     Opacity="0.1" />
    <SolidColorBrush x:Key="ControlSuccessBackgroundBrush" Color="{DynamicResource Success9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentSuccessBackgroundBrush" Color="{DynamicResource Success6Color}"
                     Opacity="0.4" />
    <SolidColorBrush x:Key="ControlTranslucentOverlaySuccessBackgroundBrush"
                     Color="{DynamicResource Success9Color}"
                     Opacity="0.1" />
    <SolidColorBrush x:Key="ControlWarningBackgroundBrush" Color="{DynamicResource Warning9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentWarningBackgroundBrush" Color="{DynamicResource Warning6Color}"
                     Opacity="0.4" />
    <SolidColorBrush x:Key="ControlTranslucentOverlayWarningBackgroundBrush"
                     Color="{DynamicResource Warning9Color}"
                     Opacity="0.1" />
    <SolidColorBrush x:Key="ControlDangerBackgroundBrush" Color="{DynamicResource Danger9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentOverlayDangerBackgroundBrush"
                     Color="{DynamicResource Danger6Color}"
                     Opacity="0.4" />
    <SolidColorBrush x:Key="ControlTranslucentDangerBackgroundBrush" Color="{DynamicResource Danger9Color}"
                     Opacity="0.1" />

    <SolidColorBrush x:Key="ControlInteractiveBackgroundBrush" Color="{DynamicResource White8Color}" />
    <SolidColorBrush x:Key="ControlInteractiveSolidBackgroundBrush" Color="{DynamicResource Gray1Color}" />
    <SolidColorBrush x:Key="ControlInteractiveBorderBrush" Color="{DynamicResource Gray6Color}" />
    <SolidColorBrush x:Key="ControlInteractiveSolidBorderBrush" Color="{DynamicResource Gray1Color}" />

    <SolidColorBrush x:Key="ControlBorderBrush" Color="{DynamicResource Gray5Color}" />
    <SolidColorBrush x:Key="ControlAccentBorderBrush" Color="{DynamicResource Accent9Color}" />
    <SolidColorBrush x:Key="ControlSuccessBorderBrush" Color="{DynamicResource Success9Color}" />
    <SolidColorBrush x:Key="ControlWarningBorderBrush" Color="{DynamicResource Warning9Color}" />
    <SolidColorBrush x:Key="ControlDangerBorderBrush" Color="{DynamicResource Danger9Color}" />

    <SolidColorBrush x:Key="ControlHighlightForegroundBrush" Color="{DynamicResource Gray1Color}" />
    <SolidColorBrush x:Key="ControlActiveForegroundBrush" Color="{DynamicResource Gray1Color}" />
    <SolidColorBrush x:Key="ControlForegroundBrush" Color="{DynamicResource Gray12Color}" />
    <SolidColorBrush x:Key="ControlTranslucentForegroundBrush" Color="{DynamicResource Gray11Color}" />
    <SolidColorBrush x:Key="ControlSecondaryForegroundBrush" Color="{DynamicResource Gray9Color}" />
    <SolidColorBrush x:Key="ControlAccentForegroundBrush" Color="{DynamicResource Accent10Color}" />
    <SolidColorBrush x:Key="ControlTranslucentAccentForegroundBrush" Color="{DynamicResource Accent11Color}" />
    <SolidColorBrush x:Key="ControlSuccessForegroundBrush" Color="{DynamicResource Success9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentSuccessForegroundBrush" Color="{DynamicResource Success11Color}" />
    <SolidColorBrush x:Key="ControlWarningForegroundBrush" Color="{DynamicResource Warning9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentWarningForegroundBrush" Color="{DynamicResource Warning11Color}" />
    <SolidColorBrush x:Key="ControlDangerForegroundBrush" Color="{DynamicResource Danger9Color}" />
    <SolidColorBrush x:Key="ControlTranslucentDangerForegroundBrush" Color="{DynamicResource Danger11Color}" />

    <SolidColorBrush x:Key="NotificationBorderBrush" Color="{DynamicResource Gray1Color}" />
    <SolidColorBrush x:Key="NotificationBackgroundBrush" Color="{DynamicResource Gray1Color}" />
    <LinearGradientBrush x:Key="NotificationSuccessBackgroundBrush" StartPoint="50%,0%" EndPoint="50%,100%">
        <GradientStops>
            <GradientStop Offset="0" Color="{DynamicResource Success4Color}" />
            <GradientStop Offset="0.3" Color="{DynamicResource Success2Color}" />
            <GradientStop Offset="0.8" Color="{DynamicResource Success1Color}" />
            <GradientStop Offset="1" Color="{DynamicResource Gray2Color}" />
        </GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="NotificationWarningBackgroundBrush" StartPoint="50%,0%" EndPoint="50%,100%">
        <GradientStops>
            <GradientStop Offset="0" Color="{DynamicResource Warning4Color}" />
            <GradientStop Offset="0.3" Color="{DynamicResource Warning2Color}" />
            <GradientStop Offset="0.8" Color="{DynamicResource Warning1Color}" />
            <GradientStop Offset="1" Color="{DynamicResource Gray2Color}" />
        </GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="NotificationDangerBackgroundBrush" StartPoint="50%,0%" EndPoint="50%,100%">
        <GradientStops>
            <GradientStop Offset="0" Color="{DynamicResource Danger4Color}" />
            <GradientStop Offset="0.3" Color="{DynamicResource Danger2Color}" />
            <GradientStop Offset="0.8" Color="{DynamicResource Danger1Color}" />
            <GradientStop Offset="1" Color="{DynamicResource Gray2Color}" />
        </GradientStops>
    </LinearGradientBrush>
</ResourceDictionary>